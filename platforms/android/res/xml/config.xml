<?xml version='1.0' encoding='utf-8'?>
<widget id="org.norwalk.baptist" version="1.0.0" versionCode="" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <name>FBC Norwalk</name>
    <description>Stay up to date with the latest blog posts and sermon recordings from FBC Norwalk		</description>
    <author email="daniel.r.heiniger@gmail.com" href="http://norwalkbaptist.org">FBC Norwalk		</author>
    <preference name="phonegap-version" value="3.7.0" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="true" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="true" />
    <preference name="ios-statusbarstyle" value="default" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="true" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="EnableViewportScale" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="true" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="SuppressesIncrementalRendering" value="true" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="internalOnly" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="ErrorUrl" value="" />
    <preference name="BackgroundColor" value="0x000000" />
    <preference name="KeepRunning" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="LoadingDialog" value="," />
    <preference name="LoadUrlTimeoutValue" value="20000" />
    <preference name="disable-cursor" value="true" />
    <gap:platform name="ios" />
    <gap:platform name="android" />
    <gap:platform name="winphone" />
    <gap:plugin name="org.apache.cordova.media" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <icon gap:platform="ios" height="72" src="icon.png" width="72" />
    <icon gap:platform="ios" height="114" src="icon.png" width="114" />
    <icon gap:platform="ios" height="144" src="icon.png" width="144" />
    <icon gap:platform="ios" height="76" src="icon.png" width="76" />
    <icon gap:platform="ios" height="120" src="icon.png" width="120" />
    <icon gap:platform="ios" height="152" src="icon.png" width="152" />
    <icon gap:density="ldpi" gap:platform="android" src="icon.png" />
    <icon gap:density="mdpi" gap:platform="android" src="icon.png" />
    <icon gap:density="hdpi" gap:platform="android" src="icon.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="icon.png" />
    <icon gap:density="xxhdpi" gap:platform="android" src="icon.png" />
    <icon gap:density="xxxhdpi" gap:platform="android" src="icon.png" />
    <icon gap:platform="winphone" src="icon.png" />
    <icon gap:platform="winphone" gap:role="background" src="icon.png" />
    <icon gap:platform="ios" height="57" src="icon.png" width="57" />
    <gap:splash gap:platform="ios" height="320" src="splash.png" width="480" />
    <gap:splash gap:platform="ios" height="960" src="splash.png" width="640" />
    <gap:splash gap:platform="ios" height="640" src="splash.png" width="960" />
    <gap:splash gap:platform="ios" height="1024 " src="splash.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="splash.png" width="1024" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="splash.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="splash.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="splash.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="splash.png" />
    <gap:splash gap:density="xxhdpi" gap:platform="android" src="splash.png" />
    <gap:splash gap:platform="winphone" src="splash.png" />
    <gap:splash gap:platform="ios" height="480" src="splash.png" width="320" />
</widget>
